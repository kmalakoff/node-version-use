{
  "name": "node-version-use",
  "version": "0.1.16",
  "description": "Cross-platform solution for using multiple versions of node. Useful for compatibility testing",
  "keywords": [
    "node",
    "version",
    "use",
    "versions",
    "compatability",
    "cross",
    "platform",
    "platforms"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-version-use.git"
  },
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "nvu": "./bin/node-version-use.js"
  },
  "scripts": {
    "compat": "./bin/node-version-use.js engines mocha-compat test/spec/**/*.test.js --no-timeouts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "dtd \"npm run lint\" \"depcheck\"",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts"
  },
  "dependencies": {
    "cross-spawn-cb": "^0.3.1",
    "exit": "^0.1.2",
    "getopts-compat": "^2.2.5",
    "node-install-release": "^0.1.25",
    "node-resolve-versions": "^0.1.5",
    "node-version-utils": "^0.1.1",
    "object-assign": "^4.1.1",
    "osenv": "^0.1.5",
    "queue-cb": "^1.1.4"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cr": "^0.1.0",
    "depcheck": "^0.9.2",
    "dis-dat": "^0.1.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "is-version": "^0.2.0",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.0.5",
    "rimraf": "^2.7.1"
  },
  "engines": {
    "node": ">=0.10"
  }
}
