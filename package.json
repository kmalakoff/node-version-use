{
  "name": "node-version-use",
  "version": "0.1.6",
  "description": "Cross-platform solution for running commands on a specific version of node",
  "keywords": [
    "node",
    "version",
    "use",
    "versions",
    "compatability",
    "cross",
    "platform",
    "platforms"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-version-use.git"
  },
  "license": "MIT",
  "main": "index.js",
  "bin": {
    "nvu": "./bin/node-version-use"
  },
  "scripts": {
    "compat": "nvs --engines -- mocha-compat test/spec/**/*.test.js  --no-timeouts",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts"
  },
  "dependencies": {
    "cross-spawn-cb": "^0.2.4",
    "end-with": "^1.0.2",
    "get-remote": "^0.4.5",
    "getopts-compat": "^2.2.5",
    "lodash.keys": "^4.2.0",
    "node-install-release": "^0.1.6",
    "node-semvers": "^0.4.4",
    "object-assign": "^4.1.1",
    "osenv": "^0.1.5"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "is-version": "^0.2.0",
    "mocha-compat": "^3.5.5",
    "nvs": "^0.4.4",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">0.12"
  }
}
