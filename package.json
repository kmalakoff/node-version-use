{
  "name": "node-version-use",
  "version": "1.0.3",
  "description": "Cross-platform solution for using multiple versions of node. Useful for compatibility testing",
  "keywords": [
    "node",
    "version",
    "use",
    "versions",
    "compatability",
    "cross",
    "platform",
    "platforms"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-version-use.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "nvu": "./bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^1.1.2",
    "exit": "^0.1.2",
    "getopts-compat": "^2.2.5",
    "homedir-polyfill": "^1.0.3",
    "node-install-release": "^1.4.1",
    "node-resolve-versions": "^1.0.0",
    "node-version-utils": "^1.0.1",
    "queue-cb": "^1.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "cr": "^0.1.0",
    "depcheck": "^1.4.7",
    "is-version": "^0.2.1",
    "rimraf2": "^2.8.2",
    "ts-dev-stack": "^1.2.1"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
