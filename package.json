{
  "name": "node-version-use",
  "version": "0.3.3",
  "description": "Cross-platform solution for using multiple versions of node. Useful for compatibility testing",
  "keywords": [
    "node",
    "version",
    "use",
    "versions",
    "compatability",
    "cross",
    "platform",
    "platforms"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/node-version-use.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "bin": {
    "nvu": "./bin/node-version-use.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "compat": "./bin/node-version-use.js engines mocha-compat test/spec/**/*.test.js --no-timeouts",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && depcheck",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts",
    "test:engines": "nvu engines npm test"
  },
  "dependencies": {
    "cross-spawn-cb": "^0.6.5",
    "exit": "^0.1.2",
    "getopts-compat": "^2.2.5",
    "just-extend": "^6.0.1",
    "node-install-release": "^0.3.4",
    "node-resolve-versions": "^0.3.2",
    "node-version-utils": "^0.5.2",
    "osenv": "^0.1.5",
    "queue-cb": "^1.1.7"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.30.7",
    "cr": "^0.1.0",
    "depcheck": "^1.4.3",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.0.0",
    "is-version": "^0.2.1",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.7.1",
    "rimraf": "^2.7.1"
  },
  "engines": {
    "node": ">=0.8"
  }
}
